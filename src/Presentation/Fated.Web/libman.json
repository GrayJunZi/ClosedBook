{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    /*
    * jQuery & Bootstrap
    */
    {
      "library": "twitter-bootstrap@5.0.0-beta2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [ "css/bootstrap.css", "css/bootstrap.css.map", "css/bootstrap.min.css", "css/bootstrap.min.css.map", "js/bootstrap.js", "js/bootstrap.js.map", "js/bootstrap.min.js", "js/bootstrap.min.js.map" ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [ "jquery.validate.js", "jquery.validate.min.js", "localization/messages_zh.js", "localization/messages_zh.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "popper.js@2.6.0",
      "destination": "wwwroot/lib/jquery-popper/",
      "files": [ "umd/popper.js", "umd/popper.js.map", "umd/popper.min.js", "umd/popper.min.js.map" ]
    },
    /*
    * Fonts & Icons 
    */
    {
      "library": "font-awesome@5.15.2",
      "destination": "wwwroot/fonts/font-awesome/"
    },
    {
      "provider": "unpkg",
      "library": "@icon/feather@4.28.0-alpha.0",
      "destination": "wwwroot/fonts/feather-icons/",
      "files": [ "feather.css", "feather.eot", "feather.svg", "feather.ttf", "feather.woff", "feather.woff2" ]
    },
    {
      "library": "izitoast@1.4.0",
      "destination": "wwwroot/assets/vendors/iziToast/"
    },
    {
      "library": "OwlCarousel2@2.3.4",
      "destination": "wwwroot/assets/vendors/OwlCarousel2/"
    },
    {
      "library": "Swiper@6.4.11",
      "destination": "wwwroot/assets/vendors/Swiper/"
    }
  ]
}